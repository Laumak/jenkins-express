{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "Simple Expressjs server with ES6 and ESLint support.",
  "main": "index.js",
  "license": "MIT",
  "author": "Lauri MÃ¤kinen",
  "scripts": {
    "open:src": "nodemon --watch src --exec \"babel-node src/index.js\"",
    "dev": "npm-run-all --parallel open:src lint:watch",
    "start": "babel-node src",
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "chalk": "^1.1.3",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.13.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "supertest": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Laumak/express-boilerplate.git"
  }
}
